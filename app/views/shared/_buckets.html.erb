<div class="tab-content">
	<div class="tab-pane fade in active" id="example-search-class-notes">
		<div class="row">
			<div class="col-sm-8 col-xs-12">
				<ul class="media-list">
				  <%for bucket in buckets%>
				  <li class="media">
						<%= render(:partial => 'shared/bucket', :locals => {:bucket => bucket } )%>
				  </li>
				  <%end%>
				  <li><%= link_to("Make A Bucket" , new_bucket_path(buckets.first.course_id) , :class => "btn btn-success") %></li>
				</ul>
			</div>
		</div>
	</div><!-- /.tab-pane fade -->
</div>
